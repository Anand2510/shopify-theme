<h1>{{ page.title }}</h1>
<div>{{ page.content }}</div>
{% schema %}
{
    "name": "page settings",
    "settings": [
        {
            "type":"select",
            "id":"container",
            "label":"layout",
            "option":[
                {
                    "value":"container",
                    "label":"container"
                },
                {
                    "value":"container-fluid",
                    "label":"container-fluid"
                }
            ],
            "default":"container"
        }
        
        ]
}
{% endschema %}
