<section class="container">
    {% for product in collections[section.settings.featured_collection].products %}
        {%product.title%}
    {% endfor %}
</section>
{% comment %} {% unless section.settings.disabled %} {% endcomment %}
    {% schema %}
    {
        "name": "Feature Collection",
        "class": "featured-collection-section",
        "settings": [
            {
                "type": "collection",
                "id": "feature_collection",
                "label": "Collection"
            },
            {
                "type": "text",
                "id": "title",
                "default": "Feature Collection",
                "label": "Title"
            }
        ],
        "presets": [
            {
                "category": "Collection",
                "name": "Feature Collection"
            }
        ]
    }
    {% endschema %}
  {% comment %} {% endunless %} {% endcomment %}